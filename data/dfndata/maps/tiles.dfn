// This file contains any tiledata overrides we wish to do with regards to buggy client data (eg ladders)

// [TILE 0x0123]
// {
// WEIGHT=		// Weight property, 0	 -> 255
// HEIGHT=		// Height property, -127 -> 128
// LAYER=		// Layer property,  -127 -> 128 (though in reality, probably only accepted values used in client)
// ANIMATION=		// Animation
// HUE=			// Unknown use, perhaps colored lighting?
// QUANTITY=		// If Weapon, Quantity is Weapon Class. If Armor, Quantity is Armor Class.
// NAME=		// Name property, less than 21 characters!


// // The following flags should only be set if you know what you are doing!!!
// // They're also not documented very well either
// // they accept either a 0 or 1, basically, on or off flags is all they are

// // BaseTile Flag 1					Alternative descriptions (InsideUO)
// ATFLOORLEVEL=	// Flag 1, bit 1 (0x01)		Background
// HOLDABLE=		// Flag 1, bit 2 (0x02)		Weapon
// SIGNGUILDBANNER=	// Flag 1, bit 3 (0x04)		Transparent
// WEBDIRTBLOOD=	// Flag 1, bit 4 (0x08)		Translucent
// WALLVERTTILE=	// Flag 1, bit 5 (0x10)		Wall
// DAMAGING=		// Flag 1, bit 6 (0x20)		Damaging
// BLOCKING=		// Flag 1, bit 7 (0x40)		Impassable
// LIQUIDWET=		// Flag 1, bit 8 (0x80)		Wet

// // BaseTile Flag 2
// UNKNOWNFLAG1=	// Flag 2, bit 1 (0x01)		Unknown
// STANDABLE=		// Flag 2, bit 2 (0x02)		Surface
// CLIMBABLE=		// Flag 2, bit 3 (0x04)		Bridge
// STACKABLE=		// Flag 2, bit 4 (0x08)		Generic/Stackable
// WINDOWARCHDOOR=	// Flag 2, bit 5 (0x10)		Window
// CANNOTSHOOTTHRU=	// Flag 2, bit 6 (0x20)		No Shoot
// DISPLAYASA=		// Flag 2, bit 7 (0x40)		Prefix A
// DISPLAYASAN=		// Flag 2, bit 8 (0x80)		Prefix An

// // BaseTile Flag 3
// DESCRIPTIONTILE=	// Flag 3, bit 1 (0x01)		Internal (things like hair, beards, etc)
// FADEWITHTRANS=	// Flag 3, bit 2 (0x02)		Foliage
// PARTIALHUE=		// Flag 3, bit 3 (0x04)		Partial Hue
// UNKNOWNFLAG2=	// Flag 3, bit 4 (0x08)		Unknown
// MAP=			// Flag 3, bit 5 (0x10)		Map
// CONTAINER=		// Flag 3, bit 6 (0x20)		Container
// EQUIPABLE=		// Flag 3, bit 7 (0x40)		Wearable
// LIGHTSOURCE=		// Flag 3, bit 8 (0x80)		LightSource

// // BaseTile Flag 4
// ANIMATED=		// Flag 4, bit 1 (0x01)		Animated
// NODIAGONAL=		// Flag 4, bit 2 (0x02)		No Diagonal
// UNKNOWNFLAG3=	// Flag 4, bit 3 (0x04)		Unknown
// WHOLEBODYITEM=	// Flag 4, bit 4 (0x08)		Armour
// WALLROOFWEAP=	// Flag 4, bit 5 (0x10)		Roof
// DOOR=		// Flag 4, bit 6 (0x20)		Door
// CLIMBABLEBIT1=	// Flag 4, bit 7 (0x40)		Stair Back
// CLIMBABLEBIT2=	// Flag 4, bit 8 (0x80)		Stair Right

// Additional flags added with High Seas expansion, purpose unknown:
// ALPHABLEND=		// Flag 5, bit 1 (0x01)
// USENEWART=		// Flag 5, bit 2 (0x02)
// ARTUSED=			// Flag 5, bit 3 (0x04)
// NOSHADOW=		// Flag 5, bit 4 (0x08)
// PIXELBLEED=		// Flag 5, bit 5 (0x10)
// PLAYANIMONCE=	// Flag 5, bit 6 (0x20)
// MULTIMOVABLE=	// Flag 5, bit 7 (0x40)

